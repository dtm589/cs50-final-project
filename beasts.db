CREATE TABLE beasts (
    name TEXT,
    size TEXT,
    AC INT,
    HP INT,
    speed INT,
    str INT,
    dex INT,
    con INT,
    int INT,
    wis INT,
    cha INT,
    vulnerabilities TEXT,
    resistances TEXT,
    senses TEXT,
    languages TEXT,
    challenge INT,
    actions TEXT,
    description TEXT,
);